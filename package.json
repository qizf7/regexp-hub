{
  "name": "regexp-hub",
  "version": "0.0.1",
  "description": "A collection of useful regex patterns",
  "main": "index.js",
  "scripts": {
    "build": "webpack --env dev && webpack --env build && npm run test",
    "dev": "webpack --progress --colors --watch --env dev",
