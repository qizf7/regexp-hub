    "expression",
    "patterns"
  ],
  "author": "qizf",
  "license": "MIT",
  "devDependencies": {
    "chai": "^4.1.2",
    "mocha": "^5.2.0",
    "webpack": "^4.16.3",
    "webpack-cli": "^3.1.0",
    "yargs": "^12.0.1"
  }
}
